{
  "manifest_version": 2,
  "name": "Diff Navigator",
  "version": "1.0",
  "description": "Navigate through highlighted changes on any page",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
